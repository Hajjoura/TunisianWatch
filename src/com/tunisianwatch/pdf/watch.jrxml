	<queryString language="SQL">
		<![CDATA[SELECT
     reclamation.`id` AS reclamation_id,
     reclamation.`idlieu` AS reclamation_idlieu,
     reclamation.`date` AS reclamation_date,
     reclamation.`heure` AS reclamation_heure,
     reclamation.`description` AS reclamation_description,
     reclamation.`titre` AS reclamation_titre,
     reclamation.`idcitoyen` AS reclamation_idcitoyen,
     reclamation.`iddomaine` AS reclamation_iddomaine,
     reclamation.`etat` AS reclamation_etat,
     lieu.`id` AS lieu_id,
     lieu.`gouvernorat` AS lieu_gouvernorat,
     lieu.`lat` AS lieu_lat,
     lieu.`lon` AS lieu_lon
FROM
     `lieu` lieu INNER JOIN `reclamation` reclamation ON lieu.`id` = reclamation.`idlieu`]]>
	</queryString>
